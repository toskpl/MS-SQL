        */

        string[] Cities = new string[] { "Warszawa", "Krakow", "Lodz", "Wroclaw", "Poznan" };
        int[] Population = new int[] { 1753977, 765320, 696503, 637683, 540372 };

        for (int i = 0; i < Cities.Length; i++)
        {
